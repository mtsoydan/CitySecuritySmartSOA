<%- include('header') -%>


<div class="bodyMon">
	<page-head icon="th-list" title="All Monitorings" />
	<div class="card mb-4">
		<div class="card-body">
			<div class="input-group ">
				<input type="text"  class="searchInput"  name="input" onkeyup="myFunction()" placeholder="Search for names.."
					title="Type in a name">
			</div>
		</div>
	</div>

	<!-- Content -->
	<div class="container-fluid flex-grow-1 container-p-y">

		<h4 class="d-flex justify-content-between align-items-center w-100 font-weight-bold py-3 mb-4">
			<div>All List</div>
			<button id ="btnNew" href="/monitoring/new" type="button"
				class="btn btn-primary rounded-pill d-block"><span class="ion ion-md-add"></span>&nbsp; New
				Monitoring</button>
		</h4>


		<div>
			<table name="table" class="table table-hover table-bordered">
				<thead>
					<tr>
						<th>Name</th>
						<th>Label</th>
						<th>Label Category</th>
						<th>Confidence</th>
					</tr>
				</thead>

				<tbody>
					<%if(jsonArray=="null"){%>
					<tr>
						<td>nan</td>
						<td>nan</td>
						<td>nan</td>
						<td>nan</td>
					</tr>
					<%}else{%>
					<%for(var i=0; i < Object.keys(jsonArray.labels).length; i++){%>
					<tr>
						<td></td>

						<td><%= jsonArray.labels[i].LabelName %></td>
						<td></td>

						<td><%= jsonArray.labels[i].confidence %></td>
					</tr>
					<%}}%>

				</tbody>
			</table>
		</div>

	</div>
	<!-- / Content -->
</div>

</div>
<%- include('footer') -%>