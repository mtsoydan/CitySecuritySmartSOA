<%- include('header') -%>


<div class="bodyMon">
  <page-head icon="th-list" title="All Monitorings" />
  <div class="card mb-4">
    <div class="card-body">
      <div class="input-group">
        <input type="text" class="searchInput" name="input" onkeyup="myFunction()" placeholder="Search for names.."
          title="Type in a name">
      </div>
    </div>
  </div>

  <!-- Content -->
  <div class="container-fluid flex-grow-1 container-p-y">

    <h4 class="d-flex justify-content-between align-items-center w-100 font-weight-bold py-3 mb-4">
      <div>All List</div>
    </h4>


    <div>
      <table name="table" class="table table-hover table-bordered">
        <thead>
          <tr>
            <th>Name</th>
            <th>Label</th>
            <th>Confidence</th>
            <th>creation Time</th>
          </tr>
        </thead>

        <tbody>
          <%if(Object.keys(jsonArray).length>0){%>
          <%for(var i=0; i < Object.keys(jsonArray).length; i++){%>
          <tr>
            <td></td>

            <td><%= jsonArray[i].label %></td>
            <td><%= jsonArray[i].confidence %></td>
            <td><%= jsonArray[i].create_at %></td>
          </tr>
          <%}}else{%>
          <tr>
            <td>nan</td>
            <td>nan</td>
            <td>nan</td>
            <td>nan</td>
          </tr>
          <%}%>

        </tbody>
      </table>
    </div>

  </div>
  <!-- / Content -->

</div>

<%- include('footer') -%>